{
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "json"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:json/recommended"
    ],
    "rules": {
