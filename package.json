  "bugs": {
    "url": "https://github.com/BibleBot/BibleBot/issues"
  },
  "homepage": "https://github.com/BibleBot/BibleBot#readme",
  "devDependencies": {
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.17.0",
    "eslint-plugin-json": "^2.1.2",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/ini": "^1.3.30",
