    "ini": "^1.3.8",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.11.11",
    "node-cron": "^2.0.3",
    "ora": "^5.2.0",
    "readline": "^1.3.0",
    "utf-8-validate": "^5.0.4",
    "zlib-sync": "^0.1.7"
  }
