{
  "name": "biblebot",
  "version": "9.0.0",
  "description": "The premier Discord bot for Bible verses.",
  "main": "dist/init.js",
  "scripts": {
    "genconf": "tsc && node ./dist/scripts/config_generator.js",
    "importdbs": "tsc && node ./dist/scripts/import_old_databases.js",
    "importlangs": "tsc && node ./dist/scripts/import_languages.js",
    "prod": "tsc && node --max_old_space_size=4096 ./dist/init.js",
    "dev": "tsc && node --trace-warnings --max_old_space_size=4096 ./dist/init.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BibleBot/BibleBot.git"
