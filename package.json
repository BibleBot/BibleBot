{
  "name": "biblebot",
  "version": "9.0.0",
  "description": "The premier Discord bot for Bible verses.",
  "main": "dist/init.js",
  "scripts": {
    "genconf": "tsc && node ./dist/scripts/config_generator.js",
    "importdbs": "tsc && node ./dist/scripts/import_old_databases.js",
    "importlangs": "tsc && node ./dist/scripts/import_languages.js",
    "prod": "export NODE_ENV=production && tsc && node --trace-warnings ./dist/init.js",
    "dev": "export NODE_ENV=dev && tsc && node --trace-warnings --inspect ./dist/init.js",
    "windev": "set NODE_ENV=dev && tsc && node --trace-warnings --inspect ./dist/init.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BibleBot/BibleBot.git"
  },
  "author": "Evangelion Ltd.",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/BibleBot/BibleBot/issues"
  },
  "homepage": "https://github.com/BibleBot/BibleBot#readme",
  "devDependencies": {
    "@floffah/node-memwatch": "github:Floffah/node-memwatch",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-plugin-json": "^2.1.2",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/ini": "^1.3.30",
    "bufferutil": "^4.0.3",
    "chalk": "^4.1.0",
    "discord.js": "^12.5.1",
    "erlpack": "github:discord/erlpack",
    "git-last-commit": "^1.0.0",
    "ini": "^1.3.6",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.11.12",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "ora": "^5.1.0",
    "readline": "^1.3.0",
    "utf-8-validate": "^5.0.4",
    "zlib-sync": "^0.1.7"
  }
}
